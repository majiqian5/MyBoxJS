    {
      "id": "Caiyun",
      "name": "彩云天气",
      "keys": [
        "@caiyun.token.caiyun",
        "@caiyun.display_location",
        "@caiyun.voice_enabled",
        "@caiyun.voice_times",
        "@caiyun.alert_voice"
      ],
      "settings": [
        {
          "id": "@caiyun.token.caiyun",
          "name": "彩云天气API Token",
          "val": null,
          "type": "text",
          "autoGrow": false,
          "rows": 2,
          "desc": "彩云天气的Token，可以自行申请或者前往频道获取"
        },
        {
          "id": "@caiyun.display_location",
          "name": "是否在日志中显示经纬度",
          "val": false,
          "type": "boolean",
          "desc": ""
        },
        {
          "id": "@caiyun.voice_enabled",
          "name": "语音播报开关",
          "val": false,
          "type": "boolean",
          "desc": "启用天气语音播报功能"
        },
        {
          "id": "@caiyun.voice_times",
          "name": "播报时段",
          "val": "[\"早上6-9点\", \"下午18-21点\"]",
          "type": "text",
          "desc": "语音播报的时段，JSON格式，可选：早上6-9点、中午12-14点、下午18-21点"
        },
        {
          "id": "@caiyun.alert_voice",
          "name": "预警播报",
          "val": true,
          "type": "boolean",
          "desc": "收到天气预警时立即语音播报"
        }
      ],
      "author": "@Peng-YM",
      "repo": "https://github.com/Peng-YM/QuanX",
      "icons": [
        "https://raw.githubusercontent.com/Orz-3/mini/master/caiyun.png",
        "https://raw.githubusercontent.com/Orz-3/task/master/caiyun.png"
      ],
      "script": "https://raw.githubusercontent.com/用户名/仓库名/master/Tasks/caiyun.js"
    }
